<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.levware.common.mappers.mart.AdminObjectSelectMapper">




	<select id="getAdminObjectSelect" parameterType="java.util.List"
		resultType="map">
		select name as OBJECT_NAME from sys.objects where type='U' and name  NOT LIKE 'TB_%' 
		<choose>
			<when test="list.size != 0">
				and name not in
				<foreach collection="list" item="item" index="index"
					separator="," open="(" close=")">
					#{item}
				</foreach>
			</when>
		</choose>
	
	
	</select>



	<!-- 객체 정보 관리 리스트 작성자 조형욱 -->
	<select id="getAdminObjectRepoColList" parameterType="String"
		resultType="map">
		
			select TABLE_NAME,COLUMN_NAME from INFORMATION_SCHEMA.COLUMNS WHERE TABLE_NAME =
		#{table_name}
		
		
	</select>

</mapper>