<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.levware.common.mappers.mart.AdminObjectSelectMapper">




	<select id="getAdminObjectSelect" parameterType="java.util.List"
		resultType="map">
		select object_name from user_objects
		where (object_type ='TABLE' or object_type ='view')
		and OBJECT_NAME  NOT LIKE 'TB_%'
		<choose>
			<when test="list.size != 0">
				and object_name not in
				<foreach collection="list" item="item" index="index"
					separator="," open="(" close=")">
					#{item}
				</foreach>
			</when>
		</choose>
	
	
	</select>



	<!-- 객체 정보 관리 리스트 작성자 조형욱 -->
	<select id="getAdminObjectRepoColList" parameterType="String"
		resultType="map">
		
			select table_name,column_name from user_tab_columns WHERE table_name =
		#{table_name}
		
		
	</select>

</mapper>