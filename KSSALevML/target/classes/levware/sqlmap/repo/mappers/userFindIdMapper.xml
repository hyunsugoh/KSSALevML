<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.levware.common.mappers.repo.UserFindIdMapper">


	
	<!-- 보조 ID로 사용자ID 찾기  작성자 조형욱-->
	<select id="getUserFindId" parameterType="String" resultType="String">
		SELECT USER_ID
        FROM TB_OLAP_USR 
        WHERE SUB_ID= #{insertSubId}
	</select>	
</mapper>